{
  "name": "angular2-tv-tracker",
  "version": "0.0.1",
  "description": "An Angular2/AngularFire2 example application.",
  "keywords": [
    "angular2",
    "webpack",
    "typescript",
    "firebase",
    "angularfire"
  ],
  "author": "Andre Giannico <andre@andregiannico.com",
  "homepage": "https://github.com/giannico/angular2-tv-tracker",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "predll:dev": "npm run clean",
    "dll:dev": "cross-env NODE_ENV=dev webpack --config webpack.dll.config.js",
    "predll:build": "npm run clean",
    "dll:build": "cross-env NODE_ENV=build webpack --config webpack.dll.config.js",
    "prebuild": "npm run dll:build",
    "build": "cross-env NODE_ENV=build webpack --progress --profile --bail",
    "predev": "path-exists ./dist || echo 'Generating DLLs' && npm run dll:dev",
    "dev": "npm run dev-server",
    "dev-server": "cross-env NODE_ENV=dev webpack-dev-server --inline --progress --port 8080",
    "lint": "tslint --force \"src/**/*.ts\""
  },
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/router": "3.1.2",
    "angular2-toaster": "1.0.2",
    "angularfire2": "2.0.0-beta.6",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "ng2-bootstrap": "1.1.16",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.5.37",
    "@types/node": "6.0.46",
    "@types/request": "0.0.32",
    "angular2-template-loader": "0.6.0",
    "assets-webpack-plugin": "3.5.0",
    "autoprefixer": "6.5.1",
    "codelyzer": "1.0.0-beta.3",
    "copy-webpack-plugin": "4.0.0",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "json-loader": "0.5.4",
    "null-loader": "0.1.1",
    "path-exists-cli": "1.0.0",
    "postcss-loader": "1.1.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "ts-helpers": "1.1.2",
    "ts-loader": "1.0.0",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.0.3",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giannico/angular2-tv-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/giannico/angular2-tv-tracker/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
