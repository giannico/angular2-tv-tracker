{
  "name": "angular2-tv-tracker",
  "version": "0.0.1",
  "description": "An Angular2/AngularFire2 example application.",
  "keywords": [
    "angular2",
    "webpack",
    "typescript",
    "firebase",
    "angularfire"
  ],
  "author": "Andre Giannico <andre@andregiannico.com",
  "homepage": "https://github.com/giannico/angular2-tv-tracker",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "lint": "tslint --force \"src/**/*.ts\"",
    "start": "npm run server"
  },
  "dependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/forms": "^2.1.0",
    "@angular/http": "^2.1.0",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@angular/router": "^3.1.0",
    "angular2-toaster": "^1.0.2",
    "angularfire2": "^2.0.0-beta.5",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "firebase": "^3.3.0",
    "ng2-bootstrap": "^1.1.14",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.0",
    "@types/node": "^6.0.45",
    "@types/request": "0.0.31",
    "angular2-template-loader": "^0.5.0",
    "autoprefixer": "^6.5.1",
    "codelyzer": "^1.0.0-beta.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.4",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^0.9.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
